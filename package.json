{
  "name": "my-expenses-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
    "docker:cleanup": "bash ./scripts/start.sh cleanup",
    "docker:up": "bash ./scripts/start.sh up",
    "docker:stop": "bash ./scripts/start.sh stop",
    "docker:ps": "bash ./scripts/start.sh ps"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^5.11.6",
    "morgan": "^1.10.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.18",
    "@types/mongoose": "^5.10.2",
    "@types/morgan": "^1.9.2",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2"
  }
}
